{
  "manifest_version": 2,
  "name": "EkstraBlock",
  "description": "Blocks the new adblock block on Ekstra Bladet.",
  "version": "1.0",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["http://ekstrabladet.dk/*"],
      "js": ["jquery.js", "ekstrablock.js"]
    }
  ]
}